class Date

instance variables
day: nat1;
month: nat1;
year: nat;

operations
public Date: nat*nat*nat ==> Date
	Date(newDay,newMonth,newYear) == (
		day := newDay;
		month := newMonth;
		year := newYear;
	)
	pre newDay < 32 and newMonth < 13;

public getDay: () ==> nat1
	getDay() == return self.day;

public getMonth: () ==> nat1
	getMonth() == return self.month;

public getYear: () ==> nat
	getYear() == return self.year;
	
end Date